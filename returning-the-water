                                                    <path id="Path_2011" data-name="Path 2011" d="M953.8,382.307c0,159.671-71.95,224.23-71.95,302.587,0,70.965,51.27,86.735,71.95,86.735s71.95-15.77,71.95-86.735C1025.752,606.537,953.8,541.978,953.8,382.307Z" fill="#005561" stroke="#005561" stroke-miterlimit="10" stroke-width="4"></path>
